import re


def solve():
    nr = 0
    file1 = open('in.txt', 'r')
    file2 = open('out.txt', 'w')
    A = file1.readlines()
    for line in A:
        interval1 = int(re.search("([0-9]*)-[0-9]*", line).group(1))
        interval2 = int(re.search("[0-9]*-([0-9]*)", line).group(1))
        letter = re.search(".*([a-zA-z]):", line).group(1)
        passw = re.search(": ([a-zA-z]*$)", line).group(1)
        interval1 -= 1
        interval2 -= 1
        if len(passw) > interval1 >= 0 and len(passw) > interval2 >= 0:
            if passw[interval1] == letter or passw[interval2] == letter:
                nr += 1
                if passw[interval1] == letter and passw[interval2] == letter:
                    nr -= 1
    print(nr)


if __name__ == '__main__':
    solve()
