def solve():
    max = 0
    file1 = open('in.txt', 'r')
    file2 = open('out.txt', 'w')
    A = file1.readlines()
    p = [0] * (127 * 7)
    for line in A:
        id = 0
        l = 0
        r = 127
        for i in range(0, 7):
            if line[i] == 'F':
                r = int((l + r) / 2)
            else:
                l = int((l + r) / 2) + 1
        id = l * 8
        l = 0
        r = 7
        for i in range(7, 10):
            if line[i] == 'L':
                r = int((l + r) / 2)
            else:
                l = int((l + r) / 2) + 1
        id += l
        p[id] = 1
    for i in range(1, (127 * 7) - 2):
        if p[i - 1] == p[i + 1] == p[i] + 1:
            print(i)
            return


if __name__ == '__main__':
    solve()
